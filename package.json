{
  "name": "wordlist-english",
  "version": "1.2.0",
  "description": "A whole lotta English words.",
  "scripts": {
    "build": "babel '*.mjs' --out-dir .",
    "watch": "babel '*.mjs' --watch --extensions .mjs --out-dir .",
    "generate": "node --experimental-modules scripts/generate.mjs",
    "prepublish": "npm run generate && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "jacksonrayhamilton/babel-plugin-transform-es2015-modules-commonjs#b0b9a88"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacksonrayhamilton/wordlist-english.git"
  },
  "keywords": [
    "word",
    "words",
    "list",
    "english",
    "american",
    "british",
    "canadian"
  ],
  "author": "Jackson Ray Hamilton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacksonrayhamilton/wordlist-english/issues"
  },
  "homepage": "https://github.com/jacksonrayhamilton/wordlist-english"
}
